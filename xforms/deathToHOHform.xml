      <instance>
        <deathToHOHform id="deathToHOHform">
          <start/>
          <end/>
          <deviceid/>
          <interviewDate/>
          <householdId/>
          <fieldWorkerId/>
          <visitId/>
          <individualId/>
          <numHHmembers/>
          <hhgroup>
            <newHOHId/>
            <newHOHFirstName/>
            <newHOHLastName/>
            <newHOHdob/>
          </hhgroup>
          <date/>
          <diagnoseddeath/>
          <whom/>
          <causeofdeathdiagnosed/>
          <causofdeathnotdiagnosed/>
          <placeOfDeath/>
          <OtherplaceOfDeath/>
          <remaining/>
          <members_count/>
          <members jr:template="">
            <membersGRP>
              <position/>
              <memberFirstName/>
              <memberLastName/>
              <memberDob/>
              <relationToNewHead/>
            </membersGRP>
          </members>
          <processedByMirth/>
          <meta>
            <instanceID/>
          </meta>
        </deathToHOHform>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/deathToHOHform/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/deathToHOHform/end" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/deathToHOHform/deviceid" type="string"/>
      <bind nodeset="/deathToHOHform/interviewDate" readonly="true()" required="true()" type="date"/>
      <bind nodeset="/deathToHOHform/householdId" required="true()" type="string"/>
      <bind nodeset="/deathToHOHform/fieldWorkerId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/deathToHOHform/visitId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/deathToHOHform/individualId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/deathToHOHform/numHHmembers" required="true()" type="int"/>
      <bind nodeset="/deathToHOHform/hhgroup" relevant="../numHHmembers &gt;= 1"/>
      <bind constraint="regex(.,'^[A-Z0-9]{12}$')" jr:constraintMsg="Please check that the Id is in the correct format" nodeset="/deathToHOHform/hhgroup/newHOHId" required="true()" type="string"/>
      <bind nodeset="/deathToHOHform/hhgroup/newHOHFirstName" required="true()" type="string"/>
      <bind nodeset="/deathToHOHform/hhgroup/newHOHLastName" required="true()" type="string"/>
      <bind nodeset="/deathToHOHform/hhgroup/newHOHdob" required="true()" type="date"/>
      <bind nodeset="/deathToHOHform/date" required="true()" type="date"/>
      <bind nodeset="/deathToHOHform/diagnoseddeath" required="true()" type="select1"/>
      <bind nodeset="/deathToHOHform/whom" relevant="selected( /deathToHOHform/diagnoseddeath , '1')" required="true()" type="select1"/>
      <bind nodeset="/deathToHOHform/causeofdeathdiagnosed" relevant="selected( /deathToHOHform/diagnoseddeath , '1')" required="true()" type="select1"/>
      <bind nodeset="/deathToHOHform/causofdeathnotdiagnosed" relevant="selected( /deathToHOHform/diagnoseddeath , '2')" required="true()" type="select1"/>
      <bind nodeset="/deathToHOHform/placeOfDeath" required="true()" type="select1"/>
      <bind nodeset="/deathToHOHform/OtherplaceOfDeath" relevant="selected( /deathToHOHform/placeOfDeath , 'OTHER')" required="true()" type="string"/>
      <bind calculate="if( /deathToHOHform/numHHmembers &gt;1, /deathToHOHform/numHHmembers  -1,0)" nodeset="/deathToHOHform/remaining" type="int"/>
      <bind calculate=" /deathToHOHform/remaining " nodeset="/deathToHOHform/members_count" readonly="true()" type="string"/>
      <bind nodeset="/deathToHOHform/members" relevant="../numHHmembers &gt;1"/>
      <bind calculate="position(..)" nodeset="/deathToHOHform/members/membersGRP/position" readonly="true()" type="string"/>
      <bind nodeset="/deathToHOHform/members/membersGRP/memberFirstName" required="true()" type="string"/>
      <bind nodeset="/deathToHOHform/members/membersGRP/memberLastName" required="true()" type="string"/>
      <bind nodeset="/deathToHOHform/members/membersGRP/memberDob" required="true()" type="date"/>
      <bind nodeset="/deathToHOHform/members/membersGRP/relationToNewHead" required="true()" type="select1"/>
      <bind nodeset="/deathToHOHform/processedByMirth" relevant="1=2" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/deathToHOHform/meta/instanceID" readonly="true()" type="string"/>
    </model>
