      <bind nodeset="/in_migration/individualInfo/partialDate" relevant=" /in_migration/migrationType  = 'EXTERNAL_INMIGRATION'" required="true()" type="select1"/>
      <bind constraint=". &lt;= today()" jr:constraintMsg="the migration date can not be in the future" nodeset="/in_migration/dateOfMigration" required="true()" type="date"/>
      <bind nodeset="/in_migration/warning" readonly="true()" relevant=" /in_migration/individualInfo/dateOfBirth &gt; /in_migration/dateOfMigration  and ../migrationType = 'EXTERNAL_INMIGRATION'" required="true()" type="string"/>
