      <bind nodeset="/in_migration/province" relevant=" /in_migration/isMozambique  = 'MOZ' and  /in_migration/migrationType  = 'EXTERNAL_INMIGRATION'" type="string"/>
      <bind nodeset="/in_migration/district" relevant=" /in_migration/isMozambique  = 'MOZ' and  /in_migration/migrationType  = 'EXTERNAL_INMIGRATION'" type="string"/>
      <bind nodeset="/in_migration/adminPost" relevant=" /in_migration/isMozambique  = 'MOZ' and  /in_migration/migrationType  = 'EXTERNAL_INMIGRATION'" type="string"/>
      <bind nodeset="/in_migration/locality" relevant=" /in_migration/isMozambique  = 'MOZ' and  /in_migration/migrationType  = 'EXTERNAL_INMIGRATION'" type="string"/>
      <bind nodeset="/in_migration/shortorlongstay" required="true()" type="select1"/>
      <bind nodeset="/in_migration/processedByMirth" readonly="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/in_migration/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="concat(&quot;Imigracao_&quot;, /in_migration/individualInfo/permId )" nodeset="/in_migration/meta/instanceName" type="string"/>
