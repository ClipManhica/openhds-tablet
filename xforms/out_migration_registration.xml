      <instance>
        <out_migration_registration id="out_migration_registration">
          <start/>
          <end/>
          <deviceid/>
          <visitId/>
          <fieldWorkerId/>
          <individualId/>
          <permId/>
          <dateOfMigration/>
          <nameOfDestination/>
          <isMozambique/>
          <countryName/>
          <province/>
          <district/>
          <adminPost/>
          <locality/>
          <timeAbsence/>
          <reasonForOutMigration/>
          <maritalChange/>
          <reasonForOutMigrationOther/>
          <shortlongterm/>
          <processedByMirth/>
          <meta>
            <instanceID/>
            <instanceName/>
          </meta>
        </out_migration_registration>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/out_migration_registration/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/out_migration_registration/end" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/out_migration_registration/deviceid" type="string"/>
      <bind nodeset="/out_migration_registration/visitId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/out_migration_registration/fieldWorkerId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/out_migration_registration/individualId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/out_migration_registration/permId" readonly="true()" required="true()" type="string"/>
      <bind constraint="(.&lt;=today() and . &gt;date(date('2012-06-01')))" jr:constraintMsg="Date of migration can not be in the future or before the start of the study" nodeset="/out_migration_registration/dateOfMigration" required="true()" type="date"/>
      <bind nodeset="/out_migration_registration/nameOfDestination" required="true()" type="string"/>
      <bind nodeset="/out_migration_registration/isMozambique" required="true()" type="select1"/>
      <bind nodeset="/out_migration_registration/countryName" relevant=" /out_migration_registration/isMozambique  = 'OTHER'" required="true()" type="string"/>
      <bind nodeset="/out_migration_registration/province" relevant=" /out_migration_registration/isMozambique  = 'MOZ'" required="true()" type="string"/>
      <bind nodeset="/out_migration_registration/district" relevant=" /out_migration_registration/isMozambique  = 'MOZ'" required="true()" type="string"/>
      <bind nodeset="/out_migration_registration/adminPost" relevant=" /out_migration_registration/isMozambique  = 'MOZ'" required="true()" type="string"/>
      <bind nodeset="/out_migration_registration/locality" relevant=" /out_migration_registration/isMozambique  = 'MOZ'" required="true()" type="string"/>
      <bind nodeset="/out_migration_registration/timeAbsence" required="true()" type="select1"/>
      <bind nodeset="/out_migration_registration/reasonForOutMigration" required="true()" type="select1"/>
      <bind nodeset="/out_migration_registration/maritalChange" relevant="../reasonForOutMigration = 'MARITAL_CHANGE'" required="true()" type="select1"/>
      <bind nodeset="/out_migration_registration/reasonForOutMigrationOther" relevant="../reasonForOutMigration = 'OTHER'" required="true()" type="string"/>
      <bind nodeset="/out_migration_registration/shortlongterm" required="true()" type="select1"/>
      <bind nodeset="/out_migration_registration/processedByMirth" readonly="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/out_migration_registration/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="concat(&quot;EmExterna_&quot;, /out_migration_registration/permId )" nodeset="/out_migration_registration/meta/instanceName" type="string"/>
    </model>
  </h:head>
