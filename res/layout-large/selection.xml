<?xml version="1.0" encoding="utf-8"?>
<ScrollView
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">
	
	<LinearLayout android:layout_width="fill_parent"
	    android:layout_height="fill_parent"
	    android:orientation="vertical" >
	    
	   <TextView
	        android:id="@+id/loginGreetingText"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:paddingTop="10dp"
	        android:paddingBottom="10dp"
	        android:textAppearance="?android:attr/textAppearanceSmall"  />
	    	
	    <Button
	        android:id="@+id/regionBtn"
	        android:layout_width="150dp"
	        android:layout_height="wrap_content"
	        android:layout_marginTop="10dp"
	        android:layout_marginLeft="20dp"
	        android:text="Select Region" /> 
	    
	    <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
	            android:layout_width="fill_parent"
	            android:layout_height="wrap_content">
	        <TableRow> 
	            <TextView
			        android:id="@+id/regionName"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:text="Region:"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
	
		    	<TextView
			        android:id="@+id/regionNameText"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:inputType="text"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
	        </TableRow>
	        <TableRow>
	            <TextView
			        android:id="@+id/regionExtId"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:text="ExtId:"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
		    
		     	<TextView
			        android:id="@+id/regionExtIdText"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:paddingBottom="10dp"
			        android:inputType="text"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
	        </TableRow> 
		</TableLayout>
		    
	    <Button
	        android:id="@+id/subRegionBtn"
	        android:layout_width="150dp"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:text="Select SubRegion" />
	    
	     <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
	            android:layout_width="fill_parent"
	            android:layout_height="wrap_content">  
	        <TableRow> 
	            <TextView
			        android:id="@+id/subRegionName"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:text="Sub Region:"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
	
		    	<TextView
			        android:id="@+id/subRegionNameText"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:inputType="text"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
	        </TableRow>
	        <TableRow>
	            <TextView
			        android:id="@+id/subRegionExtId"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:text="ExtId:"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
		    
		     	<TextView
			        android:id="@+id/subRegionExtIdText"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:paddingBottom="10dp"
			        android:inputType="text"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
	        </TableRow> 
		</TableLayout>
	    
	    <Button
	        android:id="@+id/villageBtn"
	        android:layout_width="150dp"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:text="Select Village" />
	    
	     <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
	            android:layout_width="fill_parent"
	            android:layout_height="wrap_content">  
	        <TableRow> 
	            <TextView
			        android:id="@+id/villageName"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:text="Village:"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
	
		    	<TextView
			        android:id="@+id/villageNameText"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:inputType="text"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
	        </TableRow>
	        <TableRow>
	            <TextView
			        android:id="@+id/villageExtId"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:text="ExtId:"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
		    
		     	<TextView
			        android:id="@+id/villageExtIdText"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:paddingBottom="10dp"
			        android:inputType="text"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
	        </TableRow> 
		</TableLayout>

      <Button
          android:id="@+id/roundBtn"
          android:layout_width="150dp"
          android:layout_height="wrap_content"
          android:layout_marginLeft="20dp"
          android:text="Select Round"
          android:enabled="false" />
      
         <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
             android:layout_width="fill_parent"
             android:layout_height="wrap_content">  
          <TableRow> 
              <TextView
              android:id="@+id/roundNumber"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_marginLeft="20dp"
              android:text="Round Number:"
              android:textAppearance="?android:attr/textAppearanceSmall"  />
  
          <TextView
              android:id="@+id/roundNumberText"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_marginLeft="20dp"
              android:inputType="text"
              android:textAppearance="?android:attr/textAppearanceSmall"  />
          </TableRow>
          <TableRow>
              <TextView
              android:id="@+id/roundStartDate"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_marginLeft="20dp"
              android:text="Start Date:"
              android:textAppearance="?android:attr/textAppearanceSmall"  />
        
          <TextView
              android:id="@+id/roundStartDateText"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_marginLeft="20dp"
              android:inputType="text"
              android:textAppearance="?android:attr/textAppearanceSmall"  />
        </TableRow> 
        <TableRow>
              <TextView
              android:id="@+id/roundEndDate"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_marginLeft="20dp"
              android:text="End Date:"
              android:textAppearance="?android:attr/textAppearanceSmall"  />
        
          <TextView
              android:id="@+id/roundEndDateText"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_marginLeft="20dp"
              android:paddingBottom="10dp"
              android:inputType="text"
              android:textAppearance="?android:attr/textAppearanceSmall"  />
        </TableRow> 
    </TableLayout>       
       	    	        
	    <Button
	        android:id="@+id/locationBtn"
	        android:layout_width="150dp"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:text="Select Location" />
	     
	      <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
	           android:layout_width="fill_parent"
	           android:layout_height="wrap_content">  
	        <TableRow> 
	            <TextView
			        android:id="@+id/locationExtId"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:text="ExtId:"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
	
		    	<TextView
			        android:id="@+id/locationExtIdText"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:inputType="text"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
	       	</TableRow>
	        <TableRow>
	            <TextView
			        android:id="@+id/locationName"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:text="Name:"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
		    
		     	<TextView
			        android:id="@+id/locationNameText"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:inputType="text"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
	    	</TableRow> 
	    	<TableRow>
	            <TextView
			        android:id="@+id/locationLatitude"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:text="Latitude:"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
		    
		     	<TextView
			        android:id="@+id/locationLatitudeText"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:inputType="text"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
	    	</TableRow> 
	    	<TableRow>
	            <TextView
			        android:id="@+id/locationLongitude"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:text="Longitude:"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
		    
		     	<TextView
			        android:id="@+id/locationLongitudeText"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:paddingBottom="10dp"
			        android:inputType="text"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
	    	</TableRow> 
		</TableLayout>
		
	    <Button
	        android:id="@+id/individualBtn"
	        android:layout_width="150dp"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:text="Select Individual" />
	    
		      <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
	           android:layout_width="fill_parent"
	           android:layout_height="wrap_content">  
	        <TableRow> 
	            <TextView
			        android:id="@+id/individualExtId"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:text="ExtId:"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
	
		    	<TextView
			        android:id="@+id/individualExtIdText"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:inputType="text"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
	       	</TableRow>
	        <TableRow>
	            <TextView
			        android:id="@+id/individualFirstName"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:text="First Name:"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
		    
		     	<TextView
			        android:id="@+id/individualFirstNameText"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:inputType="text"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
	    	</TableRow> 
	    	<TableRow>
	            <TextView
			        android:id="@+id/individualLastName"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:text="Last Name:"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
		    
		     	<TextView
			        android:id="@+id/individualLastNameText"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:inputType="text"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
	    	</TableRow> 
	    	<TableRow>
	            <TextView
			        android:id="@+id/individualDob"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:text="Dob:"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
		    
		     	<TextView
			        android:id="@+id/individualDobText"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:paddingBottom="10dp"
			        android:inputType="text"
			        android:textAppearance="?android:attr/textAppearanceSmall"  />
	    	</TableRow> 
		</TableLayout>
	</LinearLayout>
</ScrollView>
